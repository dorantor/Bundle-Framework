{
    "name": "phpixie/bundle-framework",
    "description": "PHPixie Bundle Framework",
    "keywords": ["framework"],
    "homepage": "http://phpixie.com",
    "type": "library",
    "license": "BSD",
    "authors": [
        {
            "name": "Roman Tsiupa",
            "email": "draconyster@gmail.com",
            "homepage": "http://dracony.org"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/dorantor/PHPixie3-Framework.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/dorantor/PHPixie3-Database.git"
        }
    ],
    "require": {
        "phpixie/bundles": "dev-dorantor@dev",
        "phpixie/framework": "dev-dorantor@dev",
        "phpixie/config": "~3.1"
    },
    "require-dev": {
        "phpixie/test": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "PHPixie\\": "src/PHPixie",
            "PHPixie\\Tests\\": "tests/PHPixie/Tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        }
    }
}
